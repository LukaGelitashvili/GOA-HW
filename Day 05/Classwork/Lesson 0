print(num + num2 + num3 +num4)
print(num + num2 + num3 + num4 + num5 + num6 + num7 + num8 + num9 + num10)

print(num * num2)
print(num * num2 * num3)
print(num * num2 * num3 * num4)

print(num / num2)
print(num / num2 / num3 / num4 / num5 / num6 / num7 / num8 / num9 / num10)
print(num / num3 / num2)

print(num // num)
print(num // num2 // num3 // num4 // num5)
print(num // num2)

